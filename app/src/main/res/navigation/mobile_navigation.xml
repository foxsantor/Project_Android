<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_market_place">

    <fragment
        android:id="@+id/navigation_market_place"
        android:name="com.example.projeecto.ui.notifications.NotificationsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_notifications">

    </fragment>

    <fragment
        android:id="@+id/navigation_bookmarks"
        android:name="com.example.projeecto.ui.home.HomeFragment"
        android:label="@string/title_bookmarks"
        tools:layout="@layout/fragment_home">


    </fragment>

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.projeecto.ui.dashboard.DashboardFragment"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_dashboard">

    </fragment>

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.projeecto.ui.home.HomeFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_home">
    </fragment>

</navigation>